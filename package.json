{
  "name": "blue-grass",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "coverage": "jest --coverage",
    "test:watch": "jest --watchAll",
    "lint": "expo lint",
    "generate": "plop --plopfile scaffolding/generators/index.cjs",
    "prepare": "husky",
    "clear-jest-cache": "jest --clearCache",
    "clean": "npx expo prebuild --clean",
    "build:android": "eas build --profile preview --platform android",
    "build:ios": "eas build --profile preview --platform ios"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "<rootDir>/jest-setup.ts",
      "<rootDir>/node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/app/'(common)'/hooks/useMockComponent/index.tsx",
      "jest-setup.ts",
      "scaffolding/",
      "<rootDir>/app/'(common)'/stores/ui/mocks.tsx"
    ],
    "moduleNameMapper": {
      "\\.svg": "<rootDir>/__mocks__/svgMock.js"
    },
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.3",
    "@gorhom/bottom-sheet": "^5.0.5",
    "@react-native-firebase/app": "^21.5.0",
    "@react-native-firebase/messaging": "^21.5.0",
    "@react-native/metro-config": "^0.75.3",
    "@react-navigation/native": "^6.0.2",
    "@shopify/flash-list": "1.6.4",
    "@tanstack/react-query": "^5.56.2",
    "axios": "^1.7.7",
    "babel-plugin-inline-import": "^3.0.0",
    "expo": "~51.0.34",
    "expo-av": "~14.0.7",
    "expo-build-properties": "~0.12.5",
    "expo-constants": "~16.0.2",
    "expo-contacts": "~13.0.5",
    "expo-dev-client": "~4.0.27",
    "expo-device": "~6.0.2",
    "expo-font": "~12.0.9",
    "expo-linear-gradient": "^14.0.1",
    "expo-linking": "~6.3.1",
    "expo-local-authentication": "~14.0.1",
    "expo-notifications": "~0.28.19",
    "expo-router": "~3.5.23",
    "expo-sharing": "~12.0.1",
    "expo-splash-screen": "~0.27.6",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-web-browser": "~13.0.3",
    "html-entities": "^2.5.2",
    "jwt-decode": "^4.0.0",
    "moment": "^2.30.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.53.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-mmkv": "3.0.0-beta.5",
    "react-native-reanimated": "~3.10.1",
    "react-native-reanimated-carousel": "^3.5.1",
    "react-native-render-html": "^6.3.4",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-shimmer-placeholder": "^2.0.9",
    "react-native-svg": "15.2.0",
    "react-native-vector-icons": "^10.2.0",
    "react-native-web": "~0.19.10",
    "react-native-webview": "13.8.6",
    "styled-components": "^6.1.13",
    "zustand": "^5.0.0-rc.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@tanstack/eslint-plugin-query": "^5.57.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.7.2",
    "@types/jest": "^29.5.13",
    "@types/react": "~18.2.45",
    "@types/react-test-renderer": "^18.0.7",
    "eslint": "^8.57.0",
    "eslint-config-expo": "^7.1.2",
    "husky": "^9.1.5",
    "jest": "^29.7.0",
    "jest-expo": "^51.0.4",
    "jest-styled-components": "^7.2.0",
    "plop": "^4.0.1",
    "react-native-svg-transformer": "^1.5.0",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.3.3"
  },
  "private": true
}
